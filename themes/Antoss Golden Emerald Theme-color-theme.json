	"tokenColors": [
		{
			"scope": [
				"comment"
			],
			"settings": {
				"foreground": "#4a6ed28c",
				"fontStyle": "italic underline"
			}
		},
				{
			"scope": [
				"emphasis",
				"markup.italic"
			],
			"settings": {
				"fontStyle": "italic underline"
			}
		},
		{
			"scope": [
				"strong",
				"markup.bold"
			],
			"settings": {
				"fontStyle": "bold underline"
			}
		},
		{
			"scope": [
				"markup.underline"
			],
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"scope": [
				"text.html.derivative",
				"source.css",
				"meta.property-value.css",
				"text.html.markdown",
				"markup.list",
				"source.c",
				"source.js"
			],
			"settings": {
				"foreground": "#00e9e5",
				"fontStyle": "underline"
			}
		},
		{
			"scope": [
				"punctuation.definition.comment.html",
				"punctuation.definition.comment.begin.css",
				"punctuation.definition.comment.end.css",
				"punctuation.definition.comment.json.comments"
			],
			"settings": {
				"foreground": "#007c85",
				"fontStyle": "italic underline"
			}
		},
		{
			"scope": [
				"punctuation.separator",
				"punctuation.terminator.rule.css",
				"support.function",
				"keyword.operator",
			],
			"settings": {
				"foreground": "#81ff61",
				"fontStyle": "underline"
			}
		},
		{
			"scope": [
				"punctuation.separator.key-value.html"
			],
			"settings": {
				"foreground": "#4ee9fed6",
				"fontStyle": "underline"
			}
		},
		{
			"scope": [
				"entity.other.attribute-name",
				"support.type.property-name",
				"markup.heading",
				"support.variable",
				"entity.name.function",
			],
			"settings": {
				"foreground": "#ffdc40",
				"fontStyle": "underline"
			}
		},
		{
			"scope": [
				"entity.name.tag"
			],
